{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "typescript-config/is-valid": "off",
    "meta-viewport": "off",
    "compat-api/html": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-size-adjust",
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 133",
    "not chrome <= 133",
    "not edge <= 133",
    "not firefox <= 135",
    "not ios_saf <= 18.3",
    "not safari <= 18.3"
  ]
}